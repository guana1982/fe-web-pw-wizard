    "scripts": {
        "ng": "ng",
        "start": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng serve --configuration development",
        "startAot": "ng serve --aot",
        "build": "node --max_old_space_size=16384 ./node_modules/@angular/cli/bin/ng build --configuration development --optimization=false --base-href /web-pw-wizard/ && node post-build.js",
        "test": "ng test --code-coverage --source-map --watch=false --browsers=ChromeHeadlessCI",
        "lint": "ng lint --fix",
        "e2e": "ng e2e fe-web-pw-wizard-e2e",
        "pretty:fix": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss}\"",
        "pretty:verify": "prettier --config ./.prettierrc --list-different \"src/{app,environments}/**/*{.ts,.html,.scss}\"",
        "analyze": "ng build --configuration=production --stats-json && webpack-bundle-analyzer ./dist/stats.json",
        "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
    },
